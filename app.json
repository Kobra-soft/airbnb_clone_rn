{
  "expo": {
    "name": "airbnb_clone_rn",
    "slug": "airbnb_clone_rn",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/airbnb_icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/airbnb-logo_adaptive_icon1.png",
        "backgroundColor": "#ffffff"
      },
      "suppressMatchedInstanceWarnings": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/airbnb_logo_favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true,
      "tscofigPaths": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "c489105b-e57c-4fb3-a590-c343b76a7a57"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/c489105b-e57c-4fb3-a590-c343b76a7a57"
    }
  }
}